<h1>Listing Organizations
  <%= link_to "<i class=\"icon-plus-sign\"></i>Add Org".html_safe, new_admin_organization_path, class: "btn pull-right" %>
</h1>


<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Name</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>Telephone</th>
    <th>Fax</th>
    <th>Contact</th>
    <th>Valid from</th>
    <th>Valid to</th>
    <th>Isactive</th>
    <th></th>
  </tr>

<% @organizations.each do |organization| %>
  <tr>
    <td><%= organization.name %></td>
    <td><%= organization.address1 %></td>
    <td><%= organization.address2 %></td>
    <td><%= organization.telephone %></td>
    <td><%= organization.fax %></td>
    <td><%= organization.contact %></td>
    <td><%= organization.valid_from %></td>
    <td><%= organization.valid_to %></td>
    <td><%= organization.isactive %></td>
    <td>
      <%= link_to "<i class=\"icon-zoom-in\"></i>show".html_safe, [:admin,organization],class: 'btn btn-small pull-right' %>
      <%= link_to "<i class=\"icon-pencil\"></i>Edit".html_safe, edit_admin_organization_path(organization),class: 'btn btn-small pull-right' %>
      <%= link_to "<i class=\"icon-trash\"></i>Destroy".html_safe, [:admin,organization],class: 'btn btn-small pull-right', method: :delete, data: { confirm: 'Are you sure?' } %>
    </td>
  </tr>
<% end %>
</table>

<br />

