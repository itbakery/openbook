    </div>
  </div>
  <div class="control-group">
    <%= f.label :email , class: "control-label" %>
    <div class="controls">
    <%= f.text_field :email %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :reservationmail , class: "control-label" %>
    <div class="controls">
    <%= f.text_field :reservationmail %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :valid_from , class: "control-label" %>
