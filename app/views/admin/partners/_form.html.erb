    <%= f.text_field :valid_from %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :valid_to , class: "control-label" %>
    <div class="controls">
    <%= f.text_field :valid_to %>
    </div>
  </div>
  <div class="controls-group">
    <div class="controls">
    <%= f.submit %>
    </div>
  </div>
<% end %>
