<h1>Listing partners
  <%= link_to "<i class=\"icon-plus-sign\"></i>Add Partner".html_safe, new_admin_partner_path, class: "btn pull-right" %>
</h1>


 <table class="table table-striped table-bordered table-condensed">

  <tr>
    <th>Name</th>
    <th>Name2</th>
    <th>Description</th>
    <th>Isactive</th>
    <th>Url</th>
    <th>Address</th>
    <th>Telephone</th>
    <th>Email</th>
    <th>Reservationmail</th>
    <th>Valid from</th>
    <th>Valid to</th>
    <th></th>
  </tr>

<% @partners.each do |partner| %>
  <tr>
    <td><%= partner.name %></td>
    <td><%= partner.name2 %></td>
    <td><%= partner.description %></td>
    <td><%= partner.isactive %></td>
    <td><%= partner.url %></td>
    <td><%= partner.address %></td>
    <td><%= partner.telephone %></td>
    <td><%= partner.email %></td>
    <td><%= partner.reservationmail %></td>
    <td><%= partner.valid_from.strftime('%a, %b %d, %Y') %></td>
    <td><%= partner.valid_to.strftime('%a,  %b %d, %Y') %></td>
    <td><%= link_to "<i class=\"icon-zoom-in\"></i>Show".html_safe, [:admin,partner] ,class: 'btn btn-small pull-right' %>
    <%= link_to "<i class=\"icon-pencil\"></i>Edit".html_safe, edit_admin_partner_path(partner) ,class: 'btn btn-small pull-right' %>
    <%= link_to "<i class=\"icon-trash\"></i>Destroy".html_safe, [:admin,partner],class: 'btn btn-small pull-right'  , method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

